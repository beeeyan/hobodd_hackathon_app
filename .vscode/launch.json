{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run dev debug",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--debug",
                "--dart-define=FLAVOR=dev"
            ]
        },
        {
            "name": "Run preview dev debug",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--debug",
                "--dart-define=FLAVOR=dev",
                "--dart-define=PREVIEW=true"
            ]
        },
        {
            "name": "Run prod debug",
            "request": "launch",
            "type": "dart",
            "args": [
                "--release",
                "--dart-define=FLAVOR=prod"
            ]
        }
    ]
}